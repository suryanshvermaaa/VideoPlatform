{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/node",
			"config": {
				"includeFiles": [
					"dist/**",
					"prisma/**",
					"node_modules/.prisma/**"
				],
				"excludeFiles": [
					".env.example",
					"tsconfig.json",
					"src/**"
				]
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "dist/index.js"
		}
	]
}